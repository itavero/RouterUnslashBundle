{
    "name": "amnl/router-unslash",
    "type": "symfony-bundle",
    "description": "This Bundle redirects user who have mistyped the URL a bit",
    "keywords": ["redirect", "ux"],
    "homepage": "http://github.com/itavero/RouterUnslashBundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Arno Moonen",
            "email": "info@arnom.nl",
            "homepage": "http://arnom.nl",
            "role": "Developer"
        }
    ],

    "require": {
        "php":                            ">=5.3.2",
        "symfony/framework-bundle":       "~2.3"
    },

    "autoload": {
        "psr-0": { "AMNL\\RouterUnslashBundle": "" }
    },

    "target-dir": "AMNL/RouterUnslashBundle",

    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
